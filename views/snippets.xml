<odoo>
  <data>
    <template id="snippet_video" name="Video snippet">
  <section class="snippet_video">
    <div class="container text-center">
      <div class="row">
        <video width="100%" controls=''>
          <source src="/theme_trucker/static/src/video/intro-vid-1.mp4" type="video/mp4"/>
          Your browser does not support HTML5 video.
        </video>
      </div>
    </div>
  </section>
</template>
<template id="place_into_bar" inherit_id="website.snippets" name="Place into bar">
  <xpath expr="//div[@id='snippet_content']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="theme_trucker.snippet_video"
       t-thumbnail="/theme_trucker/static/src/img/ui/snippet_thumb.jpg"/>
  </xpath>
</template>
